{
  "root": true,
  "parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
    "project": "./tsconfig.json",
		"ecmaFeatures": {
			"jsx": true
		}
	},
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/no-floating-promises": "error"
  }
}